{
  "name": "eatradar-server",
  "version": "0.1.0",
  "description": "eatradar server app",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config app.js dotenv_config_path=../.env",
    "start-dev": "nodemon -r dotenv/config app.js dotenv_config_path=../.env",
    "table": "node -r dotenv/config scripts/tables.js dotenv_config_path=../.env",
    "view": "node -r dotenv/config scripts/materializedViews.js dotenv_config_path=../.env",
    "seed": "node -r dotenv/config scripts/seed.js dotenv_config_path=../.env",
    "check-format": "node_modules/prettier/bin-prettier.js --check *.js **/*.js",
    "format": "node_modules/prettier/bin-prettier.js --write *.js **/*.js"
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinputera/eatradar.git"
  },
  "contributors": [
    "Justin Chia Jia Jin",
    "Kevin Putera"
  ],
  "license": "ISC"
}
